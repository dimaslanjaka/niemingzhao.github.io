<script type="application/ld+json">
  {
    "author": {
      "url": "<%- post.author.url || post.url %>",
      "name": "<%= get_author_name(page, config) %>",
      "@context": "http://schema.org",
      "@type": "Person"
    },
    "headline": "<%- post.title %>",
    "publisher": {
      "url": "<%- post.author.url || post.url %>",
      "name": "<%= get_author_name(page, config) %>",
      "@context": "http://schema.org",
      "@type": "Person"
    },
    "description": "<%= post.subtitle || post.excerpt || post.title || config.title %>",
    "datePublished": "<%= date(post.date, 'YYYY-MM-DD HH:mm:ss') %>",
    "image": "<%- get_thumbnail(page, config, theme) %>",
    "dateModified": "<%- date(post.modified || post.date, 'YYYY-MM-DD HH:mm:ss') %>",
    "@context": "http://schema.org",
    "@type": "Article"
  }
</script>
