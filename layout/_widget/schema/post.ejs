<%
let pg = typeof post !== 'undefined' ? post : page;
/*delete pg.content;
delete pg._content;
delete pg.more;
delete pg.excerpt;
delete pg.raw;
writefile("post.json", pg);*/
%>
<script type="application/ld+json">
  {
    "author": {
      "url": "<%- get_author_url(pg) || config.url %>",
      "name": "<%= get_author_name(page, config) %>",
      "@context": "http://schema.org",
      "@type": "Person"
    },
    "headline": "<%- pg.title %>",
    "publisher": {
      "url": "<%- get_author_url(page) || config.url %>",
      "name": "<%= get_author_name(pg, config) %>",
      "@context": "http://schema.org",
      "@type": "Person"
    },
    "description": "<%= JSON.stringify(pg.subtitle) %>, <%= JSON.stringify(pg.excerpt) %>, <%= pg.title %>, <%= config.title %>",
    "datePublished": "<%= date(pg.date, 'YYYY-MM-DD HH:mm:ss') %>",
    "image": "<%- get_thumbnail(pg, config, theme) %>",
    "dateModified": "<%- get_date_modified(pg) %>",
    "@context": "http://schema.org",
    "@type": "Article"
  }
</script>